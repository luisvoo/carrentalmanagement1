@page "/colour/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager
<h3>View</h3>
<FormComponent colour="colour" Disabled="true" />
@code {
    [Parameter] public int id { get; set; }
    Colour make = new Colour();
    protected async override Task OnParametersSetAsync()
    {
        make = await
       _client.GetFromJsonAsync<Colour>($"{Endpoints.ColoursEndpoint}/{id}");
    }

}
